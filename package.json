{
	"name": "tailwind-prefix",
	"displayName": "Tailwind Prefix",
	"description": "A VS Code extension to correctly prefix Tailwind CSS utility classes.",
	"icon": "images/logo.png",
	"version": "1.1.1",
	"publisher": "Sifat",
	"engines": {
		"vscode": "^1.106.0"
	},
	"repository": {
		"url": "https://github.com/sifat009/tailwind-prefix"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tailwindPrefixHelper.applyTsPrefix",
				"title": "Tailwind Prefix: Add Prefix to tailwind classes"
			},
			{
				"command": "tailwindPrefixHelper.renamePrefix",
				"title": "Tailwind Prefix: Rename Existing Prefix"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack",
		"webpack-dev": "webpack --mode development --watch"
	},
	"keywords": [
		"tailwind",
		"tailwindcss",
		"prefix",
		"vscode-extension",
		"vscode",
		"tailwind-prefix",
		"tailwind-classes",
		"prefixer",
		"tailwind-prefixer",
		"classnames",
		"css",
		"frontend"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/generator": "^7.28.5",
		"@babel/parser": "^7.28.5",
		"@babel/traverse": "^7.28.5"
	},
	"devDependencies": {
		"@types/vscode": "^1.106.1",
		"@types/webpack": "^5.28.5",
		"ts-loader": "^9.5.4",
		"vsce": "^2.15.0",
		"vscode": "^1.1.34",
		"webpack": "^5.103.0",
		"webpack-cli": "^6.0.1"
	}
}
