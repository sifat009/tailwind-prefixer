{
	"name": "tailwind-prefixer",
	"displayName": "Tailwind Prefix",
	"description": "A VS Code extension to correctly prefix Tailwind CSS utility classes.",
	"icon": "images/logo.png",
	"version": "1.0.0",
	"publisher": "Sifat",
	"engines": {
		"vscode": "^1.106.0"
	},
	"repository": {
		"url": "https://github.com/sifat009/tailwind-prefixer"
	},
	"categories": [
		"Other"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tailwindPrefixHelper.applyTsPrefix",
				"title": "Tailwind Prefixer: Add Prefix to tailwind classes"
			}
		]
	},
	"activationEvents": [
		"onCommand:tailwindPrefixHelper.applyTsPrefix"
	],
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack",
		"webpack-dev": "webpack --mode development --watch"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/generator": "^7.28.5",
		"@babel/parser": "^7.28.5",
		"@babel/traverse": "^7.28.5"
	},
	"devDependencies": {
		"@types/vscode": "^1.106.1",
		"@types/webpack": "^5.28.5",
		"ts-loader": "^9.5.4",
		"vsce": "^2.15.0",
		"vscode": "^1.1.34",
		"webpack": "^5.103.0",
		"webpack-cli": "^6.0.1"
	}
}
